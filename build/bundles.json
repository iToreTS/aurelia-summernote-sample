{
  "bundles": {
    "dist/app-build": {
      "includes": [
        "[*]",
        "*.html!text",
        "*.css!text"
      ],
      "options": {
        "inject": true,
        "minify": true
      }
    },
    "dist/aurelia": {
      "includes": [
        "core-js",
        "core-js/library/modules/$",
        "core-js/library/modules/$.string-at",
        "core-js/library/modules/$.core",
        "core-js/library/modules/$.iter-define",
        "core-js/library/modules/$.iterators",
        "core-js/library/modules/$.library",
        "core-js/library/modules/$.global",
        "core-js/library/modules/$.ctx",
        "core-js/library/modules/$.classof",
        "core-js/library/modules/$.export",
        "core-js/library/modules/$.is-object",
        "core-js/library/modules/$.an-object",
        "core-js/library/modules/$.a-function",
        "core-js/library/modules/$.strict-new",
        "core-js/library/modules/$.for-of",
        "core-js/library/modules/$.set-proto",
        "core-js/library/modules/$.same-value",
        "core-js/library/modules/$.wks",
        "core-js/library/modules/$.species-constructor",
        "core-js/library/modules/$.microtask",
        "core-js/library/modules/$.descriptors",
        "core-js/library/modules/$.redefine-all",
        "core-js/library/modules/$.set-to-string-tag",
        "core-js/library/modules/$.set-species",
        "core-js/library/modules/$.iter-detect",
        "core-js/library/modules/$.to-integer",
        "core-js/library/modules/$.defined",
        "core-js/library/modules/$.add-to-unscopables",
        "core-js/library/modules/$.iter-step",
        "core-js/library/modules/$.to-iobject",
        "core-js/library/modules/$.redefine",
        "core-js/library/modules/$.hide",
        "core-js/library/modules/$.has",
        "core-js/library/modules/$.iter-create",
        "core-js/library/modules/$.cof",
        "core-js/library/modules/$.iter-call",
        "core-js/library/modules/$.is-array-iter",
        "core-js/library/modules/$.to-length",
        "core-js/library/modules/$.shared",
        "core-js/library/modules/$.uid",
        "core-js/library/modules/$.task",
        "core-js/library/modules/$.fails",
        "core-js/library/modules/$.iobject",
        "core-js/library/modules/$.property-desc",
        "core-js/library/modules/$.invoke",
        "core-js/library/modules/$.html",
        "core-js/library/modules/$.dom-create",
        "core-js/library/modules/$.keyof",
        "core-js/library/modules/$.get-names",
        "core-js/library/modules/$.enum-keys",
        "core-js/library/modules/$.is-array",
        "core-js/library/modules/es6.object.to-string",
        "core-js/library/modules/es6.string.iterator",
        "core-js/library/modules/web.dom.iterable",
        "core-js/library/modules/es6.promise",
        "core-js/library/modules/es6.array.iterator",
        "core-js/library/modules/es6.symbol",
        "core-js/library/fn/promise",
        "core-js/library/fn/object/define-property",
        "core-js/library/fn/object/create",
        "core-js/library/fn/symbol/iterator",
        "core-js/library/fn/symbol",
        "core-js/library/fn/symbol/index",
        "babel-runtime/helpers/define-decorated-property-descriptor",
        "babel-runtime/helpers/create-decorated-class",
        "babel-runtime/helpers/class-call-check",
        "babel-runtime/regenerator",
        "babel-runtime/core-js/promise",
        "babel-runtime/core-js/object/define-property",
        "babel-runtime/regenerator/index",
        "babel-runtime/regenerator/runtime",
        "babel-runtime/core-js/symbol",
        "babel-runtime/core-js/symbol/iterator",
        "babel-runtime/core-js/object/create",
        "babel-runtime/helpers/create-class",
        "text",
        "font-awesome",
        "fetch",
        "aurelia-framework",
        "aurelia-bootstrapper",
        "aurelia-fetch-client",
        "aurelia-router",
        "aurelia-animator-css",
        "aurelia/templating-binding",
        "aurelia/templating-resources",
        "aurelia/templating-router",
        "aurelia/loader-default",
        "aurelia/history-browser",
        "aurelia/logging-console"
      ],
      "options": {
        "inject": true,
        "minify": true
      }
    },
    "dist/libs": {
      "includes": [
        "jquery",
        "bootstrap/css/bootstrap.css!text",
        "aurelia-bs-modal",
        "summernote/summernote",
        "superagent",
        "uuid"
      ],
      "options": {
        "inject": true,
        "minify": false
      }
    }
  }
}
